QCheckBox {
	color: rgb(0,0,0);
	padding: 2px;
}

QCheckBox:disabled {
	color: red;
	padding: 2px;
}

QCheckBox::hover {
	border-radius:4px;
	border-style:solid;
	padding-left: 1px;
	padding-right: 1px;
	padding-bottom: 1px;
	padding-top: 1px;
	border-width:0px;    
	border-color: rgb(0, 0, 0);
	background-color:#000000;
	color: rgb(255,255,255);
}

QCheckBox::indicator:checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: gray;
	background-color: gray;
}
QCheckBox::indicator:unchecked {

	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: gray;
	background-color: white;
}


QRadioButton {
