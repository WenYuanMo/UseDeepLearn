	 color:red;
     background: white;
 	 margin: 25px 0px 25px 0px;
}
QScrollBar::handle:vertical {
	border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 3px;
	margin: 1px 2px 1px 2px;
    background: gray;
    min-width: 20px;
}

QScrollBar::add-line:vertical {
   border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
   border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar:up-arrow:vertical, QScrollBar::down-arrow:vertical {
	border: 2px solid gray;
    width: 5px;
    height: 5px;
    background: white;
}

QScrollBar:horizontal {
    border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 0px;
	 color:red;
     background: white;
    height: 20px;
    margin: 0px 25px 0px 25px;
}
QScrollBar::handle:horizontal {
   border-color: gray;
     border-style: solid;
	 border-width:2px;
 	 border-radius: 3px;
	margin: 1px 2px 1px 2px;
    background: gray;
    min-width: 20px;
}
QScrollBar::add-line:horizontal {
  border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
  border-color: gray;
    border-style: solid;
	border-width:1px;
 	border-radius: 0px;
    background: black;
    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
   border: 2px solid gray;
    width: 5px;
    height: 5px;
    background: white;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
